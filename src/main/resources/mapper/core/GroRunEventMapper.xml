<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.battery.core.mapper.GroRunEventDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.battery.core.entity.GroRunEvent">
        <id column="ID" property="ID" />
        <result column="GroupBatteryID" property="GroupBatteryID" />
        <result column="groRunEventCode" property="groRunEventCode" />
        <result column="BegTime" property="BegTime" />
    </resultMap>

    <select id="getGroRunEventByGroupBatteryID" parameterType="int"
            resultType="com.example.battery.core.entity.GroRunEvent">
        select *
        from
            GroRunEvent as G
        where
            G.GroupBatteryID = #{GroupBatteryID}
    </select>

</mapper>
